{{>head}}
<body>
    {{>navbar data = details}}

    {{!-- {{>slider slides = sliders}} --}}

    <section class="gallery_section">
        <div class="container">
            <h3 class="py-3 underline">PHOTO GALLERY</h3>
            <div class="row">
                {{#each gallery.links}}
                <div class="col-md-3">
                    <img src="{{this.url}}" class="img-fluid pb-3 rounded" style="height: 250px; object-fit: cover;">
                </div>
                {{/each}}
            </div>
        </div>
    </section>

    {{>footer}}

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
</body>
</html>